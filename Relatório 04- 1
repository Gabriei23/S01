#include <iostream>
using namespace std;

// Função recursiva Fibonacci
int fibonacci(int n) {
    if (n == 0) {
        return 0; // Caso base 1
    } else if (n == 1) {
        return 1; // Caso base 2
    } else {
        return fibonacci(n - 1) + fibonacci(n - 2); // Recursão
    }
}

int main() {
    int n;
    cout << "Digite um numero: ";
    cin >> n;

    cout << "Fibonacci(" << n << ") = " << fibonacci(n) << endl;

    return 0;
}
